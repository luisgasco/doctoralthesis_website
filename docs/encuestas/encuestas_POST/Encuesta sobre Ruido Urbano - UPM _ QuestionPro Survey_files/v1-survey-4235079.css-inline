<div class="chatAgentInfo">
<div class="chatAgentName bold"></div>
<div class="chatAgentRole">Onboarding Expert</div>
</div>
</div>
<div class="chatAgentMsgBlock">
<div class="chatAgentMsg">Thanks for visiting. If you have any questions as you click around, I'm available here to answer them.</div>
</div>

<div class="chatInputContainer">
<div id="livePersonChatError"></div>
<div id="livePersonChat">
<form action="/a/liveperson.do" method="post" id="livePersonForm" name="ContactMessageCenterForm" style="margin:0px">
<input type="hidden" name="mode" value="launch">
<input type="hidden" name="pageMode" value="quickChat">
<div>
<textarea name="emailText" id="emailText" class="chatInput" placeholder="Type message and click START CHAT" onkeypress="if (event.which == 13 || event.keyCode == 13) {formValidate('emailText');}"></textarea>
</div>
<div id="alertMsg" style="display:none;"></div>
<div style="text-align:right; margin-bottom:5px">
<input type="button" class="startChat" value="Start Chat" onclick="formValidate('emailText');" />
</div>
</form>
</div>
<div id="formsubmitdiv"></div>
</div>
</div>
</div>

<script>
function submitForm() {
        var winl = (screen.width-750)/2;
        var wint = (screen.height-640)/2;
        newDialog = window.open('about:blank','ChatWindow', "height=600,width=370,channelmode=0,dependent=0,directories=0,fullscreen=0,location=0,menubar=0,resizable=1,scrollbars=1,status=0,toolbar=0,top="+wint + ",left=" + winl);
        document.getElementById("livePersonForm").target='ChatWindow';
        document.getElementById("livePersonForm").submit();
        $('.chatBoxcontent').hide();
        $(this).toggleClass('expand');
        return false;
    }
$('.chatBoxcontent').hide();
$('.chatBoxHeader').on('click', function(){
        document.getElementById("emailText").value="";
        $('.chatBoxcontent').toggle();
        $(this).toggleClass('expand');
        $('#alertMsg').hide();
    });

randomAgent('#random', 'png', '.chatAgentName');

function formValidate(inputid){
        var textval = document.getElementById(inputid);
        if(textval.value.trim().length > 0){
            $('#alertMsg').hide();
            submitForm();}
else{
            $(textval).addClass('required');
            $('#alertMsg').show();
            $('#alertMsg').text('Please fill out this field.');
            textval.focus();}
}
</script>
